 List<String> buOptions = new ArrayList<>();
        List<String> nsbtBatchOptions = new ArrayList<>();
        try {
            buOptions = dao.getDistinctBu();
            nsbtBatchOptions = dao.getDistinctNsbtBatchNos();
        } catch (Exception e) {
            
        }
        
        req.setAttribute("buOptions", buOptions);
        req.setAttribute("nsbtBatchOptions", nsbtBatchOptions);

  try {
            result.put("buOptions", dao.getDistinctBu());
        } catch (Exception ignored) {}
        try {
            result.put("nsbtBatchOptions", dao.getDistinctNsbtBatchNos());
